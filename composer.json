{
    "name": "ricardosierra/changelog",
    "description": "Package to enable change logs to be parsed",
    "type": "library",
    "keywords": ["change", "log", "changelog", "parser"],
    "homepage": "https://github.com/ricardosierra/changelog",
    "license": "MIT",
    "authors": [
        {
            "name": "Emlyn West",
            "email": "emlyn.west@gmail.com"
        },
        {
            "name": "Ricardo Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "symfony/console": "^4.0",
        "naneau/semver": "^0.0.7"
    },
    "require-dev": {
        "php": ">=7.0",
        "ext-json": "*",
        "codeception/codeception": "~2.0",
        "codeception/mockery-module": "*",
        "milo/github-api": "*",
        "league/flysystem": "*",
        "codegyre/robo": "^0.7.2"
    },
    "autoload": {
        "psr-4": {
            "ChangeLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChangeLog\\Stub\\": "tests/stubs"
        }
    },
    "suggest": {
        "milo/github-api": "Allows change logs to be loaded and committed to/from a github repo.",
        "league/flysystem": "Allows change logs to be read and written to pretty much everywhere."
    },
    "bin":["changelog"]
}
